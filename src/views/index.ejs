    <main>
        <h1><%= about %></h1>
        
        <section id="contenedor-productos">
            <% products.forEach((p) => { %>
            <div class="card-producto">
                <img src="https://nftcalendar.io/storage/uploads/2022/02/21/image-not-found_0221202211372462137974b6c1a.png" alt="" >
                <h3><%= p.nombre %></h3>
                <p><%= p.id %></p>
                <p>$ <%= p.precio %></p>
            </div>
            <% }) %>

        </section>
    </main>


    
